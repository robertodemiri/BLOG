<%- include("partials/header") -%>
  <h1>Home</h1>

  <p><%= homeStartingContent %></p>

  <% posts.forEach((post) => { %>
    <h1><%= post.titlePost %></h1>
    <p class="truncate"><%= post.postBody.substr(0,100) + " ... " %> <a href="<%= "/posts/" + post.titlePost %>" >Read More</a></p>
  <% }); %>

<%- include("partials/footer") -%>
